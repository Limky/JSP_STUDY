package data;

public class grade_get {

	double grade1,grade2,grade3,grade4,grade5,grade6,grade7,grade8;
	double sbjscore1,sbjscore2,sbjscore3,sbjscore4,sbjscore5,sbjscore6,sbjscore7,sbjscore8;
	double avg,n=0,sum=0;

	public grade_get(String grade1,String grade2,String grade3,
			String grade4,String grade5,String grade6,String grade7,String grade8,
			String sbjscore1,String sbjscore2,String sbjscore3,
			String sbjscore4,String sbjscore5,String sbjscore6,String sbjscore7,String sbjscore8
			
			) {
		this.grade1=Double.parseDouble("grade1")*Double.parseDouble("sbjscore1");
		this.grade2=Double.parseDouble("grade2")*Double.parseDouble("sbjscore2");
		this.grade3=Double.parseDouble("grade3")*Double.parseDouble("sbjscore3");
		this.grade4=Double.parseDouble("grade4")*Double.parseDouble("sbjscore4");
		this.grade5=Double.parseDouble("grade5")*Double.parseDouble("sbjscore5");
		this.grade6=Double.parseDouble("grade6")*Double.parseDouble("sbjscore6");
		this.grade7=Double.parseDouble("grade7")*Double.parseDouble("sbjscore7");
		this.grade8=Double.parseDouble("grade8")*Double.parseDouble("sbjscore8");
		
		if(this.grade1>100){
			this.grade1=0;
			
		}
		if(this.grade2>100){
			
			this.grade2=0;
		}
		if(this.grade3>100){
			
			this.grade3=0;
		}
		if(this.grade4>100){
			
			this.grade4=0;
		}
		if(this.grade5>100){
			
			this.grade5=0;
		}
		if(this.grade6>100){
			
			this.grade6=0;
		}
		if(this.grade7>100){
			
			this.grade7=0;
		}
		if(this.grade8>100){
			
			this.grade8=0;
		}
	
		
		this.n= Integer.parseInt(sbjscore1)+
				Integer.parseInt(sbjscore2)+
				Integer.parseInt(sbjscore3)+
				Integer.parseInt(sbjscore4)+
				Integer.parseInt(sbjscore5)+
				Integer.parseInt(sbjscore6)+
				Integer.parseInt(sbjscore7)+
				Integer.parseInt(sbjscore8);
		
		
		setAvg();
		
	}
	public double getGrade1() {
		return grade1;
	}
	public double getGrade2() {
		return grade2;
	}
	public double getGrade3() {
		return grade3;
	}
	public double getGrade4() {
		return grade4;
	}
	public double getGrade5() {
		return grade5;
	}
	public double getGrade6() {
		return grade6;
	}
	public double getGrade7() {
		return grade7;
	}
	public double getGrade8() {
		return grade8;
	}
	
	public double getAvg() {
		return avg;
	}
	private void setAvg() {
		this.avg=(grade1+grade2+grade3+grade4+grade5+grade6+grade7+grade8)/n;
	}
	
	

}

